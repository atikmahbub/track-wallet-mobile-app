{
  "expo": {
    "name": "Track Wallet",
    "slug": "trackwalletapp",
    "scheme": "trackwalletapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.reactjs.native.example.TrackWalletApp",
      "infoPlist": {
        "CFBundleDisplayName": "Track Wallet"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "org.reactjs.native.example.TrackWalletApp"
    },
    "plugins": ["expo-font", "expo-router"]
  }
}
